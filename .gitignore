.DS_Store

# Ignore Vim temporary files
/vim/.netrwhist
*.swp
*.swo

# keep those directory
/vim/swp/*
/vim/undodir/*

# ignore installed plugin via vim-plug
/vim/plugged

# Keep .gitkeep files
# To keep a directory, add a .gitkeep file to it and ignore the directory with a
# wildcard: `path/to/dir/*` this will ignore content but not director itself.
!/**/.gitkeep

# Ignore specific config
/extra
